.addAddress{
  position: absolute;
  left: 35%;
  bottom: 0rpx;
  margin: 0 auto;
  text-align: center;
}

.list{
  position: relative;
  background-color: white;
  margin-top: 2rpx;
  padding: 25rpx;
  display: flex;
  align-items: center;
  justify-content:space-between;
  min-height: 150rpx;
}

.info{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.info view:first-child{
  text-align: center;
  font-size: 35rpx;
  margin-bottom: 10rpx;
}
.info view:nth-child(2){
  font-size: 30rpx;
  margin-bottom: 10rpx;
}

.phone{
  color:gray;font-size:30rpx;margin: 0 20rpx;
}
.delete{
  position: relative;
  left: 660rpx;
  bottom: 120rpx;
  z-index: 5;
  
}

